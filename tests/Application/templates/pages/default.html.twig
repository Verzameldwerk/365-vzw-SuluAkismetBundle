{% extends 'base.html.twig' %}

{% block content %}
    <h1>{{ content.title }}</h1>

    {% if content.form %}
        <div id="sulu-form">
            {% form_theme content.form '@SuluForm/themes/dynamic.html.twig' %}
            {{ form(content.form) }}
        </div>
    {% endif %}
{% endblock %}
